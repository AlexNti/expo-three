{
  "expo": {
    "name": "Expo Three",
    "description": "A collection of Three.js examples in React Native.",
    "slug": "expo-three",
    "sdkVersion": "30.0.0",
    "version": "1.0.0",
    "privacy": "public",
    "primaryColor": "#5844ed",
    "icon": "./assets/icons/expo.png",
    "githubUrl": "https://github.com/expo/expo-three",
    "splash": {
      "image": "./assets/icons/splash.png",
      "backgroundColor": "#ffffff",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": ["assets/**/*"],
    "packagerOpts": {
      "assetExts": [
        "ttf",
        "mp4",
        "otf",
        "dae",
        "glb",
        "gltf",
        "fbx",
        "obj",
        "mtl",
        "amf",
        "3mf",
        "3ds",
        "jpg",
        "assimp",
        "fbx",
        "pmd",
        "vmd",
        "ply",
        "stl",
        "vtk",
        "vtp",
        "babylon",
        "drc",
        "pcd",
        "pack",
        "bvh",
        "x"
      ]
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expo.expothree",
      "icon": "./assets/icons/ios.png",
      "buildNumber": "1",
      "appStoreUrl": "https://itunes.apple.com/us/app/expo-three/id1401079652?ls=1&mt=8",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera for aumented reality experiences."
      }
    },
    "android": {
      "icon": "./assets/icons/android.png",
      "package": "com.expo.expothree",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.expo.expothree",
      "versionCode": 1
    }
  }
}
